<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.soft.classoa.mapper.DepartmentMapper">
  <select id="selectById" parameterType="Long" resultType="top.soft.classoa.entity.Department">
SELECT*
FROM adm_department
WHERE department_id=#{value}
